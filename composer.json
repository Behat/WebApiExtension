{
    "name": "ifxtech/web-api-extension",
    "type": "behat-extension",
    "description": "Web API extension for Behat",
    "keywords": [
        "api",
        "test"
    ],
    "homepage": "http://extensions.behat.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Łukasz Czarnołęcki",
            "email": "lukasz@czarnolecki.pl"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/Czarnolecki/WebApiExtension/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "guzzlehttp/guzzle": "^6.2",
        "phpunit/phpunit": "~5.0"
    },
    "require-dev": {
        "symfony/process": "~2.1",
        "silex/silex": "~1"
    },
    "autoload": {
        "psr-4": {
            "Behat\\WebApiExtension\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
