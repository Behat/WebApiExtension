{
    "name": "linio/behat-web-api-extension",
    "type": "behat-extension",
    "description": "Fork of behat/web-api-extension with a few changes for Linio needs",
    "keywords": ["behat", "api", "test"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/Behat/WebApiExtension/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "behat/behat": "~3.0",
        "guzzlehttp/guzzle": "~7",
        "phpunit/phpunit": ">=6.0"
    },
    "require-dev": {
        "symfony/process": "~2.1",
        "silex/silex": "~1"
    },
    "autoload": {
        "psr-4": {
            "Behat\\WebApiExtension\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
